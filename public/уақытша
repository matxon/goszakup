function doAjax(url, mes_id, data, fun_ok) {
  
  var mes_obj = $('#' + mes_id);
  
  if (data == undefined) data = {};
  
  $.ajax({
    type: "POST", async: false, cache: false, url: url, data: data,
    success: function (data, textStatus, jqXHR) {
      
      var msg = data;
      
      if (mes_obj.size() != 0) {
        var h = msg;
        // h = h.replace(/\\n/g, '<br>');
        mes_obj.html(h);
      } else {
        Mes(msg);
      }
      
      if (fun_ok != undefined) {
        fun_ok(data);
      }
      
    },
    error: function (jqXHR, textStatus, errorThrown) {
      err_handler(mes_obj, jqXHR, textStatus, errorThrown)
    }
  });
}
